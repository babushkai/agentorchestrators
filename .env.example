# Environment
ENVIRONMENT=development

# NATS
NATS_SERVERS=nats://localhost:4222

# PostgreSQL
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_USER=orchestrator
DATABASE_PASSWORD=orchestrator_dev
DATABASE_NAME=agent_orchestrator

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# S3/MinIO
S3_ENDPOINT_URL=http://localhost:9000
S3_ACCESS_KEY_ID=minioadmin
S3_SECRET_ACCESS_KEY=minioadmin
S3_BUCKET=agent-orchestrator

# LLM Providers
LLM_DEFAULT_PROVIDER=anthropic
LLM_DEFAULT_MODEL=claude-sonnet-4-20250514
LLM_ANTHROPIC_API_KEY=your-anthropic-api-key
LLM_OPENAI_API_KEY=your-openai-api-key

# Telemetry
OTEL_ENABLED=true
OTEL_SERVICE_NAME=agent-orchestrator
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317

# API
API_HOST=0.0.0.0
API_PORT=8000
API_DEBUG=true
